&i2c6 {
	status = "okay";

        eeprom@51 {
                compatible = "atmel,24c64";
                reg = <0x50>;
                pagesize = <32>;
        };

        i2c-switch@70 {
                compatible = "nxp,pca9546";
                #address-cells = <1>;
                #size-cells = <0>;
                reg = <0x70>;

                /* 12c-21 */
                fan_bd1: i2c@0 {
                        #address-cells = <1>;
                        #size-cells = <0>;
                        reg = <0>;

                        eeprom@51 {
                                compatible = "atmel,24c64";
                                reg = <0x51>;
                                pagesize = <32>;
                        };

                        adt7462@58 {
                                compatible = "adt7462";
                                reg = <0x58>;
                        };

                        //PWR MON INA220
                        ina220@44 {
                                compatible = "ti,ina220";
                                reg = <0x44>;
                                shunt-resistor = <200>;
                        };

                        //PWR MON INA220
                        ina220@45 {
                                compatible = "ti,ina220";
                                reg = <0x45>;
                                shunt-resistor = <200>;
                        };

                        //Fan Thermal Sensor
                        tmp75@48 {
                               compatible = "ti,tmp75";
                               reg = <0x48>;
                        };

                        //PWR MON INA220
                        ina220@4c {
                                compatible = "ti,ina220";
                                reg = <0x4c>;
                                shunt-resistor = <200>;
                        };

                        //PWR MON INA220
                        ina220@4d {
                                compatible = "ti,ina220";
                                reg = <0x4d>;
                                shunt-resistor = <200>;
                        };
                };

                /* i2c-22 */
                i2c6_mux_3: i2c@3 {
                        #address-cells = <1>;
                        #size-cells = <0>;
                        reg = <3>;

                        tmp75@48 {
                               compatible = "ti,tmp75";
                               reg = <0x48>;
                        };
                };
        };

};

&i2c7 {
	status = "okay";

        i2c-switch@70 {
                compatible = "nxp,pca9546";
                #address-cells = <1>;
                #size-cells = <0>;
                reg = <0x70>;

		/* i2c-23 */
		i2c7_mux_0: i2c@0 {

			#address-cells = <1>;
			#size-cells = <0>;
			reg = <0>;

			/* XDPE19284B - PVCCIN VR */
			xdpe152c4@60 {
				compatible = "infineon,xdpe152c4";
				reg = <0x60>;
			};	
			
			/* XDPE19284B - PVCCFA EHV FIVRA */
			xdpe152c4@62 {
				compatible = "infineon,xdpe152c4";
				reg = <0x62>;
			};	
			
			/* XDPE19284B - PVCCD0 & D1 VR */
			xdpe152c4@76 {
				compatible = "infineon,xdpe152c4";
				reg = <0x76>;
			};	
			
			/* XDPE19284B - PVCCA EHV PVNN VR */
			xdpe152c4@74 {
				compatible = "infineon,xdpe152c4";
				reg = <0x74>;
			};

		};
		i2c7_mux_1: i2c@1 {
                        #address-cells = <1>;
                        #size-cells = <0>;
                        reg = <1>;
	            		//Voltage Sensor MAX1139
	                        adc_u74: voltage-sensor@35 {
	                                compatible = "maxim,max1139";
	                                reg = <0x35>;
	                                #io-channel-cells = <1>;
	                        };
			};
	};
};

