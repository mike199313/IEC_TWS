&fmc {
	fmc-spi-user-mode;
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_fwqspi_default>;
	timing-calibration-disabled;
	status = "okay";
	num-cs = <1>;
	flash@0 {
		status = "okay";
		m25p,fast-read;
		label = "bmc";
		spi-max-frequency = <33000000>;
		spi-tx-bus-width = <2>;
		spi-rx-bus-width = <2>;
		partitions {
			compatible = "fixed-partitions";
			#address-cells = <0x1>;
			#size-cells = <0x1>;

			u-boot@0 {
				reg = <0x0 0xe0000>;
				label = "u-boot";
			};

			u-boot-env@e0000 {
				reg = <0xe0000 0x20000>;
				label = "u-boot-env";
			};

			kernel@100000 {
				reg = <0x100000 0x900000>;
				label = "kernel";
			};

			rofs@a00000 {
				reg = <0xa00000 0x2000000>;
				label = "rofs";
			};

			rwfs@2a00000 {
				reg = <0x2a00000 0x1600000>;
				label = "rwfs";
			};
		};
	};
};

